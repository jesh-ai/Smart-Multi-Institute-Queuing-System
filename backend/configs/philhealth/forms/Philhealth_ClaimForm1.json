{
  "institutions": [
    {
      "id": "philhealth",
      "name": "Philippine Health Insurance Corporation",
      "code": "PHILHEALTH",
      "description": "Government corporation responsible for implementing the National Health Insurance Program in the Philippines",
      "forms": [
        {
          "id": "philhealth_cf1",
          "formNumber": "CF-1",
          "title": "Claim Form 1",
          "officialTitle": "Republic of the Philippines - PHILIPPINE HEALTH INSURANCE CORPORATION",
          "description": "PhilHealth Claim Form for health benefit availment",
          "category": "Health Insurance Claims",
          "version": "Revised September 2018",
          "pages": 2,
          "totalFields": 45,
          "sections": [
            {
              "id": "form_header",
              "name": "Form Header",
              "fields": [
                {
                  "id": "form_notice",
                  "fieldNumber": "notice",
                  "label": "This form may be reproduced and is NOT FOR SALE",
                  "fieldType": "notice_text",
                  "required": false
                },
                {
                  "id": "form_number",
                  "fieldNumber": "form_no",
                  "label": "CF-1 (Claim Form 1)",
                  "fieldType": "form_number",
                  "required": false
                },
                {
                  "id": "version",
                  "fieldNumber": "version",
                  "label": "Revised September 2018",
                  "fieldType": "version",
                  "required": false
                },
                {
                  "id": "series_number",
                  "fieldNumber": "series",
                  "label": "Series #",
                  "fieldType": "text",
                  "required": false,
                  "adminOnly": true
                }
              ]
            },
            {
              "id": "important_reminders",
              "name": "Important Reminders",
              "fields": [
                {
                  "id": "reminder_1",
                  "fieldNumber": "rem1",
                  "label": "PLEASE WRITE IN CAPITAL LETTERS AND CHECK THE APPROPRIATE BOXES.",
                  "fieldType": "reminder",
                  "required": false
                },
                {
                  "id": "reminder_2",
                  "fieldNumber": "rem2",
                  "label": "For local availment, this form together with other PhilHealth claim forms and other supporting documents should be filed within 60 days from date of discharge.",
                  "fieldType": "reminder",
                  "required": false
                },
                {
                  "id": "reminder_3",
                  "fieldNumber": "rem3",
                  "label": "For availment of benefits abroad, this form together with other supporting documents should be filed within 180 days from date of discharge.",
                  "fieldType": "reminder",
                  "required": false
                },
                {
                  "id": "reminder_4",
                  "fieldNumber": "rem4",
                  "label": "Representative of the Health Care Institutions (HCI) shall assist the member/authorized representative in filling out this form.",
                  "fieldType": "reminder",
                  "required": false
                },
                {
                  "id": "reminder_5",
                  "fieldNumber": "rem5",
                  "label": "All information required in this form are necessary. Claim forms with incomplete information shall not be processed.",
                  "fieldType": "reminder",
                  "required": false
                },
                {
                  "id": "reminder_6",
                  "fieldNumber": "rem6",
                  "label": "FALSE/INCORRECT INFORMATION OR MISREPRESENTATION SHALL BE SUBJECT TO CRIMINAL, CIVIL OR ADMINISTRATIVE LIABILITIES.",
                  "fieldType": "warning_text",
                  "required": false
                }
              ]
            },
            {
              "id": "part_i_member_information",
              "name": "PART I - MEMBER INFORMATION",
              "fields": [
                {
                  "id": "member_pin",
                  "fieldNumber": "1",
                  "label": "1. PhilHealth Identification Number (PIN) of Member:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"minLength": 12, "maxLength": 14},
                  "pattern": "[0-9]{12,14}"
                },
                {
                  "id": "member_name",
                  "fieldNumber": "2",
                  "label": "2. Name of Member:",
                  "fieldType": "name_extended",
                  "required": true,
                  "components": [
                    {
                      "id": "last_name",
                      "label": "Last Name",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"minLength": 1, "maxLength": 50}
                    },
                    {
                      "id": "first_name",
                      "label": "First Name",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"minLength": 1, "maxLength": 50}
                    },
                    {
                      "id": "name_extension",
                      "label": "Name Extension (JR/SR/III)",
                      "fieldType": "select",
                      "required": false,
                      "options": [
                        {"value": "jr", "label": "JR"},
                        {"value": "sr", "label": "SR"},
                        {"value": "ii", "label": "II"},
                        {"value": "iii", "label": "III"},
                        {"value": "iv", "label": "IV"}
                      ]
                    },
                    {
                      "id": "middle_name",
                      "label": "Middle Name",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    }
                  ],
                  "example": "ex: DELA CRUZ JUAN JR SIPAG"
                },
                {
                  "id": "member_birth_date",
                  "fieldNumber": "3",
                  "label": "3. Date of Birth:",
                  "fieldType": "date_parts",
                  "required": true,
                  "components": [
                    {
                      "id": "birth_month",
                      "label": "Month",
                      "fieldType": "select",
                      "required": true,
                      "options": [
                        {"value": "01", "label": "January"}, {"value": "02", "label": "February"}, {"value": "03", "label": "March"},
                        {"value": "04", "label": "April"}, {"value": "05", "label": "May"}, {"value": "06", "label": "June"},
                        {"value": "07", "label": "July"}, {"value": "08", "label": "August"}, {"value": "09", "label": "September"},
                        {"value": "10", "label": "October"}, {"value": "11", "label": "November"}, {"value": "12", "label": "December"}
                      ]
                    },
                    {
                      "id": "birth_day",
                      "label": "Day",
                      "fieldType": "number",
                      "required": true,
                      "min": 1,
                      "max": 31
                    },
                    {
                      "id": "birth_year",
                      "label": "Year",
                      "fieldType": "number",
                      "required": true,
                      "min": 1900,
                      "max": 2024
                    }
                  ]
                },
                {
                  "id": "member_mailing_address",
                  "fieldNumber": "4",
                  "label": "4. Mailing Address:",
                  "fieldType": "philhealth_address",
                  "required": true,
                  "components": [
                    {
                      "id": "unit_room_floor",
                      "label": "Unit/Room No./Floor",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "building_name",
                      "label": "Building Name",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "lot_block_house_bldg_no",
                      "label": "Lot/Blk/House/Bldg.No",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "street",
                      "label": "Street",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "subdivision_village",
                      "label": "Subdivision/Village",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "barangay",
                      "label": "Barangay",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "city_municipality",
                      "label": "City/Municipality",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "province",
                      "label": "Province",
                      "fieldType": "text",
                      "required": true,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "country",
                      "label": "Country",
                      "fieldType": "text",
                      "required": true,
                      "defaultValue": "Philippines",
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "zip_code",
                      "label": "Zip Code",
                      "fieldType": "text",
                      "required": true,
                      "pattern": "[0-9]{4}",
                      "validation": {"maxLength": 4}
                    }
                  ]
                },
                {
                  "id": "member_sex",
                  "fieldNumber": "5",
                  "label": "5. Sex:",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "male", "label": "Male"},
                    {"value": "female", "label": "Female"}
                  ]
                },
                {
                  "id": "member_contact_information",
                  "fieldNumber": "6",
                  "label": "6. Contact Information:",
                  "fieldType": "contact_group",
                  "required": true,
                  "components": [
                    {
                      "id": "landline_no",
                      "label": "Landline No. (Area Code + Tel. No.)",
                      "fieldType": "tel",
                      "required": false,
                      "pattern": "[0-9]{7,10}"
                    },
                    {
                      "id": "mobile_no",
                      "label": "Mobile No.",
                      "fieldType": "tel",
                      "required": true,
                      "pattern": "09[0-9]{9}"
                    },
                    {
                      "id": "email_address",
                      "label": "Email Address",
                      "fieldType": "email",
                      "required": false,
                      "validation": {"maxLength": 100}
                    }
                  ]
                },
                {
                  "id": "patient_is_member",
                  "fieldNumber": "7",
                  "label": "7. Patient is the member?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes, Proceed to Part III"},
                    {"value": "no", "label": "No, Proceed to Part II"}
                  ]
                }
              ]
            },
            {
              "id": "part_ii_patient_information",
              "name": "PART II - PATIENT INFORMATION",
              "instructions": "To be filled-out only if the patient is a dependent",
              "fields": [
                {
                  "id": "dependent_pin",
                  "fieldNumber": "1",
                  "label": "1. PhilHealth Identification Number (PIN) of Dependent:",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 14},
                  "conditional": {
                    "show": "patient_is_member == 'no'"
                  }
                },
                {
                  "id": "patient_name",
                  "fieldNumber": "2",
                  "label": "2. Name of Patient:",
                  "fieldType": "name_extended",
                  "required": false,
                  "components": [
                    {
                      "id": "last_name",
                      "label": "Last Name",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "first_name",
                      "label": "First Name",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    },
                    {
                      "id": "name_extension",
                      "label": "Name Extension (JR/SR/III)",
                      "fieldType": "select",
                      "required": false,
                      "options": [
                        {"value": "jr", "label": "JR"},
                        {"value": "sr", "label": "SR"},
                        {"value": "ii", "label": "II"},
                        {"value": "iii", "label": "III"},
                        {"value": "iv", "label": "IV"}
                      ]
                    },
                    {
                      "id": "middle_name",
                      "label": "Middle Name",
                      "fieldType": "text",
                      "required": false,
                      "validation": {"maxLength": 50}
                    }
                  ],
                  "example": "ex: DELA CRUZ JUAN JR SIPAG",
                  "conditional": {
                    "show": "patient_is_member == 'no'"
                  }
                },
                {
                  "id": "patient_birth_date",
                  "fieldNumber": "3",
                  "label": "3. Date of Birth:",
                  "fieldType": "date_parts",
                  "required": false,
                  "components": [
                    {
                      "id": "birth_month",
                      "label": "Month",
                      "fieldType": "select",
                      "required": false
                    },
                    {
                      "id": "birth_day",
                      "label": "Day",
                      "fieldType": "number",
                      "required": false,
                      "min": 1,
                      "max": 31
                    },
                    {
                      "id": "birth_year",
                      "label": "Year",
                      "fieldType": "number",
                      "required": false,
                      "min": 1900,
                      "max": 2024
                    }
                  ],
                  "conditional": {
                    "show": "patient_is_member == 'no'"
                  }
                },
                {
                  "id": "relationship_to_member",
                  "fieldNumber": "4",
                  "label": "4. Relationship to Member:",
                  "fieldType": "radio",
                  "required": false,
                  "options": [
                    {"value": "child", "label": "Child"},
                    {"value": "parent", "label": "Parent"},
                    {"value": "spouse", "label": "Spouse"}
                  ],
                  "conditional": {
                    "show": "patient_is_member == 'no'"
                  }
                },
                {
                  "id": "patient_sex",
                  "fieldNumber": "5",
                  "label": "5. Sex:",
                  "fieldType": "radio",
                  "required": false,
                  "options": [
                    {"value": "male", "label": "Male"},
                    {"value": "female", "label": "Female"}
                  ],
                  "conditional": {
                    "show": "patient_is_member == 'no'"
                  }
                }
              ]
            },
            {
              "id": "part_iii_member_certification",
              "name": "PART III - MEMBER CERTIFICATION",
              "fields": [
                {
                  "id": "certification_text",
                  "fieldNumber": "cert_text",
                  "label": "Under the penalty of law, I attest that the information I provided in this Form are true and accurate to the best of my knowledge.",
                  "fieldType": "certification_text",
                  "required": true,
                  "defaultValue": true
                },
                {
                  "id": "member_signature",
                  "fieldNumber": "member_sig",
                  "label": "Signature Over Printed Name of Member",
                  "fieldType": "signature",
                  "required": false,
                  "conditional": {
                    "show": "signing_person == 'member'"
                  }
                },
                {
                  "id": "member_signature_date",
                  "fieldNumber": "member_sig_date",
                  "label": "Date Signed",
                  "fieldType": "date_parts",
                  "required": false,
                  "conditional": {
                    "show": "signing_person == 'member'"
                  }
                },
                {
                  "id": "representative_signature",
                  "fieldNumber": "rep_sig",
                  "label": "Signature Over Printed Name of Member's Representative",
                  "fieldType": "signature",
                  "required": false,
                  "conditional": {
                    "show": "signing_person == 'representative'"
                  }
                },
                {
                  "id": "representative_signature_date",
                  "fieldNumber": "rep_sig_date",
                  "label": "Date Signed",
                  "fieldType": "date_parts",
                  "required": false,
                  "conditional": {
                    "show": "signing_person == 'representative'"
                  }
                },
                {
                  "id": "signing_person",
                  "fieldNumber": "signing_person",
                  "label": "If member/representative is unable to write, put right thumbmark. Member/Representative should be assisted by an HCI representative. Check the appropriate box.",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "member", "label": "Member"},
                    {"value": "representative", "label": "Representative"}
                  ]
                },
                {
                  "id": "representative_relationship",
                  "fieldNumber": "rep_relationship",
                  "label": "Relationship of the representative to the member",
                  "fieldType": "radio",
                  "required": false,
                  "options": [
                    {"value": "spouse", "label": "Spouse"},
                    {"value": "child", "label": "Child"},
                    {"value": "parent", "label": "Parent"},
                    {"value": "sibling", "label": "Sibling"},
                    {"value": "others", "label": "Others, Specify"}
                  ],
                  "conditional": {
                    "show": "signing_person == 'representative'"
                  }
                },
                {
                  "id": "other_relationship",
                  "fieldNumber": "other_relationship",
                  "label": "Other Relationship Specification",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "representative_relationship == 'others'"
                  }
                },
                {
                  "id": "signing_reason",
                  "fieldNumber": "signing_reason",
                  "label": "Reason for signing on behalf of the member",
                  "fieldType": "radio",
                  "required": false,
                  "options": [
                    {"value": "incapacitated", "label": "Member is incapacitated"},
                    {"value": "other_reasons", "label": "Other reasons"}
                  ],
                  "conditional": {
                    "show": "signing_person == 'representative'"
                  }
                },
                {
                  "id": "other_reasons",
                  "fieldNumber": "other_reasons",
                  "label": "Other Reasons Specification",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "signing_reason == 'other_reasons'"
                  }
                }
              ]
            },
            {
              "id": "part_iv_employer_certification",
              "name": "PART IV - EMPLOYER'S CERTIFICATION",
              "instructions": "for employed members only",
              "fields": [
                {
                  "id": "employer_pen",
                  "fieldNumber": "1",
                  "label": "1. PhilHealth Employer Number (PEN):",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 12}
                },
                {
                  "id": "employer_contact_no",
                  "fieldNumber": "2",
                  "label": "2. Contact No.:",
                  "fieldType": "tel",
                  "required": false
                },
                {
                  "id": "employer_business_name",
                  "fieldNumber": "3",
                  "label": "3. Business Name:",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 150}
                },
                {
                  "id": "employer_certification_text",
                  "fieldNumber": "4",
                  "label": "4. CERTIFICATION OF EMPLOYER: \"This is to certify that the required 3/6 monthly premium contributions plus at least 6 months contributions preceding the 3 months qualifying contributions within 12 month period prior to the first day of confinement (sufficient regularity) have been regularly remitted to PhilHealth. Moreover, the information supplied by the member or his/her representative on Part I are consistent with our available records.\"",
                  "fieldType": "certification_text",
                  "required": false
                },
                {
                  "id": "employer_signature",
                  "fieldNumber": "employer_sig",
                  "label": "Signature Over Printed Name of Employer/Authorized Representative",
                  "fieldType": "signature",
                  "required": false
                },
                {
                  "id": "employer_official_capacity",
                  "fieldNumber": "employer_capacity",
                  "label": "Official Capacity/Designation",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 50}
                },
                {
                  "id": "employer_signature_date",
                  "fieldNumber": "employer_sig_date",
                  "label": "Date Signed",
                  "fieldType": "date_parts",
                  "required": false
                }
              ]
            },
            {
              "id": "part_v_philhealth_use",
              "name": "PART V - FOR PHILHEALTH USE ONLY",
              "adminOnly": true,
              "fields": [
                {
                  "id": "date_received",
                  "fieldNumber": "date_received",
                  "label": "Date Received:",
                  "fieldType": "date",
                  "required": false,
                  "adminOnly": true,
                  "format": "MM/DD/YYYY"
                },
                {
                  "id": "lhio_pro",
                  "fieldNumber": "lhio_pro",
                  "label": "LHIO PRO",
                  "fieldType": "text",
                  "required": false,
                  "adminOnly": true
                },
                {
                  "id": "received_by",
                  "fieldNumber": "received_by",
                  "label": "By:",
                  "fieldType": "signature",
                  "required": false,
                  "adminOnly": true,
                  "instructions": "LHIO/PRO Signature Over Printed Name"
                }
              ]
            }
          ],
          "validationRules": {
            "memberInformation": [
              {
                "rule": "Member PIN is required and must be valid",
                "condition": "member_pin IS NOT EMPTY AND LENGTH(member_pin) >= 12"
              },
              {
                "rule": "Member name (last and first) is required",
                "condition": "member_last_name IS NOT EMPTY AND member_first_name IS NOT EMPTY"
              },
              {
                "rule": "Date of birth is required",
                "condition": "member_birth_month IS NOT EMPTY AND member_birth_day IS NOT EMPTY AND member_birth_year IS NOT EMPTY"
              },
              {
                "rule": "Complete address is required",
                "condition": "lot_block_house_bldg_no IS NOT EMPTY AND street IS NOT EMPTY AND barangay IS NOT EMPTY AND city_municipality IS NOT EMPTY AND province IS NOT EMPTY AND zip_code IS NOT EMPTY"
              },
              {
                "rule": "Sex and contact information are required",
                "condition": "member_sex IS NOT EMPTY AND mobile_no IS NOT EMPTY"
              }
            ],
            "dependentInformation": [
              {
                "rule": "If patient is dependent, dependent information is required",
                "condition": "patient_is_member == 'no' AND patient_last_name IS NOT EMPTY AND patient_first_name IS NOT EMPTY AND patient_birth_month IS NOT EMPTY AND patient_birth_day IS NOT EMPTY AND patient_birth_year IS NOT EMPTY AND relationship_to_member IS NOT EMPTY AND patient_sex IS NOT EMPTY"
              }
            ],
            "certificationRequirements": [
              {
                "rule": "Member certification must be signed",
                "condition": "signing_person IS NOT EMPTY AND ((signing_person == 'member' AND member_signature IS NOT EMPTY) OR (signing_person == 'representative' AND representative_signature IS NOT EMPTY AND representative_relationship IS NOT EMPTY AND signing_reason IS NOT EMPTY))"
              }
            ],
            "filingDeadlines": [
              {
                "rule": "Local claims must be filed within 60 days from discharge",
                "condition": "date_received <= date_of_discharge + 60 days"
              },
              {
                "rule": "Foreign claims must be filed within 180 days from discharge",
                "condition": "country != 'Philippines' AND date_received <= date_of_discharge + 180 days"
              }
            ]
          },
          "businessRules": {
            "filingDeadlines": {
              "local": "60 days from date of discharge",
              "foreign": "180 days from date of discharge"
            },
            "premiumRequirements": {
              "employed": "3/6 monthly premium contributions plus at least 6 months contributions preceding the 3 months qualifying contributions within 12 month period prior to confinement",
              "self_employed": "Regular premium payments as required"
            },
            "eligibleDependents": [
              "Legal spouse",
              "Children below 21 years old",
              "Children 21+ with disabilities",
              "Parents 60+ years old"
            ]
          },
          "documentaryRequirements": {
            "general": [
              "Valid ID of member/patient",
              "Proof of relationship (for dependents)",
              "Medical records and discharge summary",
              "Official receipt of premium payments"
            ],
            "specific": [
              "Birth certificate (for dependents)",
              "Marriage certificate (for spouse)",
              "Medical certificate for disabilities",
              "Employer certification (for employed members)"
            ]
          }
        }
      ]
    }
  ],
