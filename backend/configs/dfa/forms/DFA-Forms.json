{
  "institutions": [
    {
      "id": "dfa",
      "name": "Department of Foreign Affairs",
      "code": "DFA",
      "description": "Government agency responsible for foreign relations and passport issuance in the Philippines",
      "forms": [
        {
          "id": "dfa_passport_application",
          "formNumber": "DFA-OCA-PDO-02",
          "title": "Passport Application Form",
          "officialTitle": "Republic of the Philippines - DEPARTMENT OF FOREIGN AFFAIRS - PASSPORT APPLICATION FORM",
          "description": "Official passport application form for Philippine citizens",
          "category": "Passport Services",
          "version": "02/27 DEC 2024",
          "pages": 1,
          "totalFields": 45,
          "sections": [
            {
              "id": "form_header",
              "name": "Form Header",
              "fields": [
                {
                  "id": "form_notice",
                  "fieldNumber": "notice",
                  "label": "THIS APPLICATION FORM IS NOT FOR SALE. PLEASE DO NOT LEAVE ANY SPACES BLANK, INDICATE N/A IF NOT APPLICABLE. PROVIDING FALSE STATEMENTS IN PASSPORT APPLICATIONS IS PUNISHABLE BY LAW (R.A. 11983).",
                  "fieldType": "warning_text",
                  "required": false
                }
              ]
            },
            {
              "id": "application_type_payment",
              "name": "Application Type and Payment",
              "fields": [
                {
                  "id": "application_type",
                  "fieldNumber": "app_type",
                  "label": "APPLICATION TYPE:",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "adult", "label": "ADULT"},
                    {"value": "minor", "label": "MINOR (below 18 years of age)"}
                  ]
                },
                {
                  "id": "order_of_payment",
                  "fieldNumber": "payment_type",
                  "label": "ORDER OF PAYMENT: (FOR USE OF THE DFA ONLY.)",
                  "fieldType": "radio",
                  "required": true,
                  "adminOnly": true,
                  "options": [
                    {"value": "expedite", "label": "EXPEDITE (₱ 250.00)"},
                    {"value": "regular", "label": "REGULAR (₱ 950.00)"},
                    {"value": "lost_damaged", "label": "LOST/DAMAGED (₱ 1,300.00)"},
                    {"value": "others", "label": "OTHERS"}
                  ]
                },
                {
                  "id": "other_payment_amount",
                  "fieldNumber": "other_payment",
                  "label": "Other Payment Amount",
                  "fieldType": "currency",
                  "required": false,
                  "adminOnly": true,
                  "conditional": {
                    "show": "order_of_payment == 'others'"
                  }
                }
              ]
            },
            {
              "id": "personal_information",
              "name": "Personal Information",
              "fields": [
                {
                  "id": "last_name",
                  "fieldNumber": "last_name",
                  "label": "LAST NAME / APELYIDO",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"minLength": 1, "maxLength": 50}
                },
                {
                  "id": "first_name",
                  "fieldNumber": "first_name",
                  "label": "FIRST NAME / PANGALAN",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"minLength": 1, "maxLength": 50}
                },
                {
                  "id": "middle_name",
                  "fieldNumber": "middle_name",
                  "label": "MIDDLE NAME / GITNANG PANGALAN",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 50}
                },
                {
                  "id": "suffix",
                  "fieldNumber": "suffix",
                  "label": "Suffix",
                  "fieldType": "select",
                  "required": false,
                  "options": [
                    {"value": "jr", "label": "JR."},
                    {"value": "ii", "label": "II"},
                    {"value": "iii", "label": "III"},
                    {"value": "iv", "label": "IV"},
                    {"value": "v", "label": "V"}
                  ]
                },
                {
                  "id": "place_of_birth",
                  "fieldNumber": "pob",
                  "label": "PLACE OF BIRTH / POOK NG KAPANGANAKAN",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 100},
                  "placeholder": "e.g. MAKATI MM or EGYPT"
                },
                {
                  "id": "date_of_birth",
                  "fieldNumber": "dob",
                  "label": "DATE OF BIRTH / PETSA NG KAPANGANAKAN",
                  "fieldType": "date_parts",
                  "required": true,
                  "components": [
                    {
                      "id": "birth_day",
                      "label": "DAY",
                      "fieldType": "number",
                      "required": true,
                      "min": 1,
                      "max": 31
                    },
                    {
                      "id": "birth_month",
                      "label": "MONTH",
                      "fieldType": "select",
                      "required": true,
                      "options": [
                        {"value": "01", "label": "January"}, {"value": "02", "label": "February"}, {"value": "03", "label": "March"},
                        {"value": "04", "label": "April"}, {"value": "05", "label": "May"}, {"value": "06", "label": "June"},
                        {"value": "07", "label":  "July"}, {"value": "08", "label": "August"}, {"value": "09", "label": "September"},
                        {"value": "10", "label": "October"}, {"value": "11", "label": "November"}, {"value": "12", "label": "December"}
                      ]
                    },
                    {
                      "id": "birth_year",
                      "label": "YEAR",
                      "fieldType": "number",
                      "required": true,
                      "min": 1900,
                      "max": 2024
                    }
                  ]
                },
                {
                  "id": "sex",
                  "fieldNumber": "sex",
                  "label": "SEX / KASARIAN",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "male", "label": "MALE"},
                    {"value": "female", "label": "FEMALE"}
                  ]
                },
                {
                  "id": "national_id_no",
                  "fieldNumber": "national_id",
                  "label": "NATIONAL ID NO.",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 20}
                },
                {
                  "id": "civil_status",
                  "fieldNumber": "civil_status",
                  "label": "Civil Status:",
                  "fieldType": "select",
                  "required": true,
                  "options": [
                    {"value": "single", "label": "Single"},
                    {"value": "married", "label": "Married"},
                    {"value": "widow", "label": "Widow/er"},
                    {"value": "legally_separated", "label": "Legally Separated"},
                    {"value": "annulled", "label": "Annulled"}
                  ]
                },
                {
                  "id": "present_address",
                  "fieldNumber": "address",
                  "label": "Present Address:",
                  "fieldType": "textarea",
                  "required": true,
                  "validation": {"maxLength": 200}
                },
                {
                  "id": "contact_number",
                  "fieldNumber": "contact",
                  "label": "Contact no.:",
                  "fieldType": "tel",
                  "required": true,
                  "pattern": "09[0-9]{9}"
                },
                {
                  "id": "email_address",
                  "fieldNumber": "email",
                  "label": "Email address:",
                  "fieldType": "email",
                  "required": true,
                  "validation": {"maxLength": 100}
                }
              ]
            },
            {
              "id": "family_information",
              "name": "Family Information",
              "fields": [
                {
                  "id": "spouse_name",
                  "fieldNumber": "spouse",
                  "label": "Spouse's Name:",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 100},
                  "conditional": {
                    "show": "civil_status == 'married'"
                  }
                },
                {
                  "id": "spouse_citizenship",
                  "fieldNumber": "spouse_citizen",
                  "label": "Spouse's citizenship:",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 50},
                  "conditional": {
                    "show": "civil_status == 'married'"
                  }
                },
                {
                  "id": "fathers_name",
                  "fieldNumber": "father",
                  "label": "Father's Name:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 100}
                },
                {
                  "id": "father_citizenship",
                  "fieldNumber": "father_citizen",
                  "label": "Father's citizenship:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 50}
                },
                {
                  "id": "mother_maiden_name",
                  "fieldNumber": "mother",
                  "label": "Mother's Maiden Name:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 100}
                },
                {
                  "id": "mother_citizenship",
                  "fieldNumber": "mother_citizen",
                  "label": "Mother's citizenship:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 50}
                }
              ]
            },
            {
              "id": "citizenship_information",
              "name": "Citizenship Information",
              "fields": [
                {
                  "id": "citizenship_acquired_by",
                  "fieldNumber": "citizen_acquired",
                  "label": "Citizenship Acquired By:",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "birth", "label": "Birth"},
                    {"value": "election", "label": "Election"},
                    {"value": "naturalization", "label": "Naturalization"},
                    {"value": "ra_9225", "label": "R.A. 9225"},
                    {"value": "others", "label": "Others"}
                  ]
                },
                {
                  "id": "other_citizenship_acquisition",
                  "fieldNumber": "other_citizen",
                  "label": "Other Citizenship Acquisition",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "citizenship_acquired_by == 'others'"
                  }
                },
                {
                  "id": "foreign_citizenship_acquired",
                  "fieldNumber": "foreign_citizen",
                  "label": "Have you ever acquired foreign citizenship?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes"},
                    {"value": "no", "label": "No"}
                  ]
                },
                {
                  "id": "foreign_country",
                  "fieldNumber": "foreign_country",
                  "label": "If YES, from what country?",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "foreign_citizenship_acquired == 'yes'"
                  }
                },
                {
                  "id": "mode_of_acquisition",
                  "fieldNumber": "acquisition_mode",
                  "label": "Mode of acquisition:",
                  "fieldType": "radio",
                  "required": false,
                  "options": [
                    {"value": "birth", "label": "Birth"},
                    {"value": "naturalization", "label": "Naturalization"},
                    {"value": "others", "label": "Others"}
                  ],
                  "conditional": {
                    "show": "foreign_citizenship_acquired == 'yes'"
                  }
                },
                {
                  "id": "other_acquisition_mode",
                  "fieldNumber": "other_mode",
                  "label": "Other Mode",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "mode_of_acquisition == 'others'"
                  }
                }
              ]
            },
            {
              "id": "military_service",
              "name": "Military Service Information",
              "fields": [
                {
                  "id": "foreign_military_service",
                  "fieldNumber": "military_service",
                  "label": "Have you served in any foreign military?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes"},
                    {"value": "no", "label": "No"}
                  ]
                },
                {
                  "id": "military_country",
                  "fieldNumber": "military_country",
                  "label": "If YES, from what country?",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "foreign_military_service == 'yes'"
                  }
                }
              ]
            },
            {
              "id": "citizenship_renunciation",
              "name": "Citizenship Renunciation",
              "fields": [
                {
                  "id": "ph_citizenship_renounced",
                  "fieldNumber": "renounced",
                  "label": "Have you renounced your Philippine Citizenship?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes"},
                    {"value": "no", "label": "No"}
                  ]
                }
              ]
            },
            {
              "id": "passport_history",
              "name": "Passport History",
              "fields": [
                {
                  "id": "foreign_passport_issued",
                  "fieldNumber": "foreign_passport",
                  "label": "Have you been issued a foreign passport?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes"},
                    {"value": "no", "label": "No"}
                  ]
                },
                {
                  "id": "foreign_passport_country",
                  "fieldNumber": "foreign_passport_country",
                  "label": "If YES, from what country?",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "foreign_passport_issued == 'yes'"
                  }
                },
                {
                  "id": "ph_passport_issued",
                  "fieldNumber": "ph_passport",
                  "label": "Have you ever been issued a Philippine passport?",
                  "fieldType": "radio",
                  "required": true,
                  "options": [
                    {"value": "yes", "label": "Yes"},
                    {"value": "no", "label": "No"}
                  ]
                },
                {
                  "id": "latest_passport_number",
                  "fieldNumber": "passport_no",
                  "label": "If YES, latest passport number?",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 20},
                  "conditional": {
                    "show": "ph_passport_issued == 'yes'"
                  }
                },
                {
                  "id": "passport_issue_date",
                  "fieldNumber": "issue_date",
                  "label": "Date of issue:",
                  "fieldType": "date",
                  "required": false,
                  "format": "MM/DD/YYYY",
                  "conditional": {
                    "show": "ph_passport_issued == 'yes'"
                  }
                },
                {
                  "id": "passport_issue_place",
                  "fieldNumber": "issue_place",
                  "label": "Place of issue:",
                  "fieldType": "text",
                  "required": false,
                  "validation": {"maxLength": 50},
                  "conditional": {
                    "show": "ph_passport_issued == 'yes'"
                  }
                }
              ]
            },
            {
              "id": "declaration_affirmation",
              "name": "Declaration and Affirmation",
              "fields": [
                {
                  "id": "declaration_text",
                  "fieldNumber": "declaration",
                  "label": "I HEREBY DECLARE AND AFFIRM that (1) I am a Filipino citizen, (2) the information I provided in this application are true and correct, (3) the supporting documents are authentic, (4) I am aware that under the law, I am allowed to hold only one Philippine passport at any given time, (5) I am aware that making false statements in passport application, furnishing falsified or forged documents in support thereof are punishable by law.",
                  "fieldType": "declaration_text",
                  "required": true,
                  "defaultValue": true
                },
                {
                  "id": "privacy_policy",
                  "fieldNumber": "privacy",
                  "label": "PRIVACY POLICY: I understand that I am giving my unequivocal consent to the disclosure, collection, and use of my personal information and the data required under the New Passport Law and its Implementing Rules and Regulations. My consent effectively constitutes a waiver of any and all privacy rights pertaining to the disclosure, collection, and use of my personal information and data under the specific terms and condition found in the DFA Online Passport Appointment System Website's Privacy Policy and other pertinent DFA rules, regulations, policies on the matter, including the sharing of the above information with other Philippine Government agencies, subject to the Data Privacy Act of 2012. I authorize the DFA to publish my First and Last Name in the DFA website for the purpose of official advisory that my passport is available for release.",
                  "fieldType": "privacy_consent",
                  "required": true,
                  "defaultValue": true
                },
                {
                  "id": "acknowledgment",
                  "fieldNumber": "acknowledge",
                  "label": "BY SIGNING BELOW, I acknowledge that I have carefully read this application form and fully understand its contents. I hereby voluntarily agree that if I am unable to attend my scheduled passport appointment or if my passport application is denied due to lack of documentary requirements or other reasons, my payment for the passport application will be forfeited and non-refundable.",
                  "fieldType": "acknowledgment_text",
                  "required": true,
                  "defaultValue": true
                },
                {
                  "id": "applicant_signature",
                  "fieldNumber": "signature",
                  "label": "Signature of Applicant or Parent / Legal Guardian (for minor applicants)",
                  "fieldType": "signature",
                  "required": true
                }
              ]
            },
            {
              "id": "minor_applicant_section",
              "name": "For Minor Applicants",
              "fields": [
                {
                  "id": "accompanying_adult",
                  "fieldNumber": "accompanying",
                  "label": "Accompanied by:",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "application_type == 'minor'"
                  }
                },
                {
                  "id": "relationship_to_applicant",
                  "fieldNumber": "relationship",
                  "label": "Relationship to the applicant:",
                  "fieldType": "text",
                  "required": false,
                  "conditional": {
                    "show": "application_type == 'minor'"
                  }
                },
                {
                  "id": "accompanying_mobile",
                  "fieldNumber": "accompanying_mobile",
                  "label": "Mobile No. (Accompanying adult):",
                  "fieldType": "tel",
                  "required": false,
                  "conditional": {
                    "show": "application_type == 'minor'"
                  }
                }
              ]
            },
            {
              "id": "emergency_contact",
              "name": "Emergency Contact",
              "fields": [
                {
                  "id": "emergency_contact_name",
                  "fieldNumber": "emergency_name",
                  "label": "Name of Emergency Contact:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 100}
                },
                {
                  "id": "emergency_mobile",
                  "fieldNumber": "emergency_mobile",
                  "label": "Mobile No.:",
                  "fieldType": "tel",
                  "required": true
                },
                {
                  "id": "emergency_relationship",
                  "fieldNumber": "emergency_relationship",
                  "label": "Relationship:",
                  "fieldType": "text",
                  "required": true,
                  "validation": {"maxLength": 50}
                },
                {
                  "id": "emergency_address",
                  "fieldNumber": "emergency_address",
                  "label": "Present Address:",
                  "fieldType": "textarea",
                  "required": true,
                  "validation": {"maxLength": 200}
                }
              ]
            },
            {
              "id": "dfa_admin_section",
              "name": "DFA Administrative Section",
              "adminOnly": true,
              "fields": [
                {
                  "id": "remarks",
                  "fieldNumber": "remarks",
                  "label": "REMARKS:",
                  "fieldType": "textarea",
                  "required": false,
                  "adminOnly": true
                },
                {
                  "id": "received_canceled_passport",
                  "fieldNumber": "canceled_passport",
                  "label": "RECEIVED CANCELED PASSPORT:",
                  "fieldType": "text",
                  "required": false,
                  "adminOnly": true
                },
                {
                  "id": "wl_verification",
                  "fieldNumber": "wl_verification",
                  "label": "WL VERIFICATION:",
                  "fieldType": "text",
                  "required": false,
                  "adminOnly": true
                },
                {
                  "id": "staff_signatures",
                  "fieldNumber": "staff_sigs",
                  "label": "Staff Signatures",
                  "fieldType": "signature_group",
                  "required": false,
                  "adminOnly": true,
                  "components": [
                    {
                      "id": "verifier",
                      "label": "Verifier:",
                      "fieldType": "signature",
                      "required": false
                    },
                    {
                      "id": "processor",
                      "label": "Processor:",
                      "fieldType": "signature",
                      "required": false
                    },
                    {
                      "id": "encoder",
                      "label": "Encoder:",
                      "fieldType": "signature",
                      "required": false
                    },
                    {
                      "id": "signing_officer",
                      "label": "Signing Officer:",
                      "fieldType": "signature",
                      "required": false
                    },
                    {
                      "id": "transmission_officer",
                      "label": "Transmission Officer:",
                      "fieldType": "signature",
                      "required": false
                    }
                  ]
                }
              ]
            }
          ],
          "validationRules": {
            "personalInformation": [
              {
                "rule": "Last name and first name are required",
                "condition": "last_name IS NOT EMPTY AND first_name IS NOT EMPTY"
              },
              {
                "rule": "Date of birth must be valid",
                "condition": "birth_day IS NOT EMPTY AND birth_month IS NOT EMPTY AND birth_year IS NOT EMPTY"
              },
              {
                "rule": "Place of birth is required",
                "condition": "place_of_birth IS NOT EMPTY"
              }
            ],
            "familyInformation": [
              {
                "rule": "Father's and mother's information is required",
                "condition": "fathers_name IS NOT EMPTY AND father_citizenship IS NOT EMPTY AND mother_maiden_name IS NOT EMPTY AND mother_citizenship IS NOT EMPTY"
              }
            ],
            "conditionalRequirements": [
              {
                "rule": "If married, spouse information is required",
                "condition": "civil_status == 'married' AND spouse_name IS NOT EMPTY AND spouse_citizenship IS NOT EMPTY"
              },
              {
                "rule": "If foreign citizenship acquired, country must be specified",
                "condition": "foreign_citizenship_acquired == 'yes' AND foreign_country IS NOT EMPTY"
              },
              {
                "rule": "If Philippine passport issued, details must be provided",
                "condition": "ph_passport_issued == 'yes' AND latest_passport_number IS NOT EMPTY"
              }
            ],
            "documentRequirements": [
              {
                "rule": "All declarations and consents must be accepted",
                "condition": "declaration_text == true AND privacy_policy == true AND acknowledgment == true"
              },
              {
                "rule": "Applicant signature is required",
                "condition": "applicant_signature IS NOT EMPTY"
              }
            ]
          },
          "legalReferences": [
            "R.A. 11983 - New Passport Law",
            "R.A. 9225 - Citizenship Retention and Re-acquisition Act of 2003",
            "Data Privacy Act of 2012"
          ],
          "feeStructure": {
            "expedite": 1300.00,
            "regular": 950.00,
            "lost_damaged": 250.00
          },
          "documentaryRequirements": {
            "adult_new": [
              "Birth Certificate (PSA)",
              "Valid ID",
              "Marriage Certificate (if applicable)",
              "Other supporting documents as required"
            ],
            "minor": [
              "Birth Certificate (PSA)",
              "Marriage Certificate of Parents",
              "Valid IDs of Parents",
              "School ID (if applicable)"
            ],
            "renewal": [
              "Current/Expired Passport",
              "Valid ID",
              "Other supporting documents as required"
            ]
          }
        }
      ]
    }
  ],
  "metadata": {
    "version": "1.0",
    "lastUpdated": "2024-01-15",
    "formats": {
      "date": "MM/DD/YYYY",
      "phone": "09XXXXXXXXX",
      "currency": "PHP"
    },
    "website": "https://consular.dfa.gov.ph/"
  }
}