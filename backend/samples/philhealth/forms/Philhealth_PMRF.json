              {
                "rule": "Mother's maiden name is required",
                "condition": "mother_maiden_name IS NOT EMPTY"
              },
              {
                "rule": "Date of birth and place of birth are required",
                "condition": "birth_date_month IS NOT EMPTY AND birth_date_day IS NOT EMPTY AND birth_date_year IS NOT EMPTY AND birth_place IS NOT EMPTY"
              },
              {
                "rule": "Sex and civil status are required",
                "condition": "sex IS NOT EMPTY AND civil_status IS NOT EMPTY"
              },
              {
                "rule": "Citizenship is required",
                "condition": "citizenship_type IS NOT EMPTY"
              }
            ],
            "addressContact": [
              {
                "rule": "Complete permanent address is required",
                "condition": "permanent_lot_block_phase_house_no IS NOT EMPTY AND permanent_street_name IS NOT EMPTY AND permanent_barangay IS NOT EMPTY AND permanent_municipality_city IS NOT EMPTY AND permanent_province_state_country IS NOT EMPTY AND permanent_zip_code IS NOT EMPTY"
              },
              {
                "rule": "Mobile number is required",
                "condition": "mobile_number IS NOT EMPTY"
              },
              {
                "rule": "Email is required for OFW members",
                "condition": "(member_type contains 'migrant_worker') ? email_address IS NOT EMPTY : true"
              }
            ],
            "memberType": [
